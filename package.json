{
  "author": "Alekseev Aleksey",
  "name": "spx-com-dev",
  "app-name": "spx",
  "version": "1.1.3",
  "description": "Client Object model for Sharepoint 2013",
  "main": "publish/umd/index.min.js",
  "private": false,
  "scripts": {
    "serve": "node dev/init.js && webpack-dev-server --open --config dev/webpack.dev.js",
    "copy": "node dev/copy.js",
    "patch-publish": "cd publish && npm version patch && npm publish",
    "publish-prepare": "rollup -c dev/rollup.config.js && node dev/copy-assets.js",
    "deploy": "npm run publish-prepare && node dev/deploy.js",
    "test": "jest"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ne4to777/spx-com.git"
  },
  "keywords": [
    "sharepoint",
    "jsom",
    "rest"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ne4to777/spx-com/issues"
  },
  "homepage": "https://github.com/Ne4to777/spx-com#readme",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.17.2",
    "jest": "^24.8.0",
    "rollup": "^1.25.1",
    "webpack-cli": "latest",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "axios": ">=0.18.1",
    "babel-jest": "^24.9.0",
    "cpass": "^2.0.4",
    "crypto-js": "^3.1.9-1",
    "html-webpack-plugin": "^3.2.0",
    "robocopy": "^0.1.16",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "sp-rest-proxy": "^2.8.10",
    "webpack": "^4.39.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "modules": false,
          "targets": {
            "browsers": "> 0.05%, not ie < 10, not op_mini all",
            "node": "current"
          }
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/env",
            {
              "modules": false
            }
          ]
        ],
        "plugins": [
          "@babel/plugin-transform-modules-commonjs",
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "<rootDir>/dev/setupFiles/",
      "<rootDir>/publish/"
    ],
    "setupFiles": [
      "<rootDir>/dev/setupFiles/sp.assembly.js"
    ],
    "testURL": "http://localhost:3000/"
  },
  "eslintConfig": {
    "env": {
      "jest": true
    },
    "globals": {
      "SP": "readonly",
      "Microsoft": "readonly",
      "window": "readonly"
    }
  }
}